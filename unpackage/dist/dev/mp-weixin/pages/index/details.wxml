<view class="parameterBox"><view class="parameterBox_main"><view class="top dispaly"><view class="left">基本</view><view class="right"><view><text class="left_title">编号：</text><text>{{info.deviceSn}}</text></view><view><text class="left_title">用户：</text><text>{{info.fireUnitName}}</text></view><view class="dispaly margin"><text class="left_title">联系人：</text><input class="uni-input" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['$0','contractName','$event',[]],['info']]]]]}}" value="{{info.contractName}}" bindinput="__e"/></view><view class="dispaly"><text class="left_title">手机号：</text><input class="uni-input" placeholder="请输入电话号码" data-event-opts="{{[['input',[['__set_model',['$0','contractPhone','$event',[]],['info']]]]]}}" value="{{info.contractPhone}}" bindinput="__e"/></view></view></view><view class="bottom dispaly"><view class="left">参数</view><view class="right"><view class="range"><view class="bottom_right_title dispaly"><image src="../../static/image/tip.png" mode></image><text>监测范围</text></view><view class="dispaly" style="width:60%;"><text class="bottom_left_title">剩余电流：</text><input class="uni-input inputwidth" placeholder="自动获得焦点" data-event-opts="{{[['input',[['__set_model',['$0','maxAmpere','$event',[]],['info']]]]]}}" value="{{info.maxAmpere}}" bindinput="__e"/>mA</view><view class="dispaly margintb"><text class="bottom_left_title">电缆温度：</text><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="radioBox" bindchange="__e"><label class="radio" style="margin-right:30rpx;"><radio color="#409eff" value="1" checked="{{info.phaseType==1}}"></radio>单相</label><label class="radio"><radio color="#409eff" checked="{{info.phaseType==2}}" value="2"></radio>三相</label></radio-group></view><block wx:if="{{info.phaseType==1}}"><view><view class="dispaly" style="margin-bottom:10rpx;width:60%;"><text class="bottom_left_title">L：</text><input class="uni-input" placeholder="L温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxL','$event',[]],['info']]]]]}}" value="{{info.maxL}}" bindinput="__e"/>℃</view><view class="dispaly" style="width:60%;"><text class="bottom_left_title">N：</text><input class="uni-input" placeholder="N温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxN','$event',[]],['info']]]]]}}" value="{{info.maxN}}" bindinput="__e"/>℃</view></view></block><block wx:if="{{info.phaseType==2}}"><view><view class="dispaly" style="margin-bottom:10rpx;width:60%;"><text class="bottom_left_title">L1：</text><input class="uni-input" placeholder="L温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxL1','$event',[]],['info']]]]]}}" value="{{info.maxL1}}" bindinput="__e"/>℃</view><view class="dispaly" style="width:60%;"><text class="bottom_left_title">L2：</text><input class="uni-input" placeholder="N温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxL2','$event',[]],['info']]]]]}}" value="{{info.maxL2}}" bindinput="__e"/>℃</view><view class="dispaly" style="width:60%;"><text class="bottom_left_title">L3：</text><input class="uni-input" placeholder="N温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxL3','$event',[]],['info']]]]]}}" value="{{info.maxL3}}" bindinput="__e"/>℃</view><view class="dispaly" style="width:60%;"><text class="bottom_left_title">N：</text><input class="uni-input" placeholder="N温度上限" data-event-opts="{{[['input',[['__set_model',['$0','maxN','$event',[]],['info']]]]]}}" value="{{info.maxN}}" bindinput="__e"/>℃</view></view></block></view><view><view class="bottom_right_title dispaly"><image src="../../static/image/tip.png" mode></image><text>超限动作</text></view><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event']]]]]}}" class="margintb" bindchange="__e"><label><checkbox style="transform:scale(0.9);" value="自动断电" checked="{{info.enableAlarmSwitch}}" color="#409eff"></checkbox>自动断电</label></checkbox-group><view class="messageBox dispaly"><checkbox style="transform:scale(0.9);" value="发送短信" checked="{{info.enableSMS}}" color="#409eff" data-event-opts="{{[['tap',[['changeenableSMS',['$event']]]]]}}" bindtap="__e"></checkbox>发送短信<block wx:if="{{info.enableSMS}}"><input class="uni-input" placeholder="自动获得焦点" data-event-opts="{{[['input',[['__set_model',['$0','smsPhones','$event',[]],['info']]]]]}}" value="{{info.smsPhones}}" bindinput="__e"/></block></view></view></view></view><view class="btnBox"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></view></view></view>