.parameterBox {
  width: 100%;
}
.parameterBox .parameterBox_main {
  width: 100%;
}
.parameterBox .parameterBox_main .dispaly {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.parameterBox .parameterBox_main .margin {
  margin-bottom: 10rpx;
}
.parameterBox .parameterBox_main .margintb {
  margin: 20rpx 0rpx;
}
.parameterBox .parameterBox_main .top {
  width: 100%;
  padding: 30rpx 10rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #F0F0F0;
}
.parameterBox .parameterBox_main .left {
  font-size: 30rpx;
  font-weight: bold;
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 auto;
          flex: 0 0 auto;
  padding: 0px 20rpx;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.parameterBox .parameterBox_main .right {
  padding: 0px 10rpx 0rpx 20rpx;
  border-left: 1rpx solid #F0F0F0;
}
.parameterBox .parameterBox_main .right .left_title {
  display: inline-block;
  width: 120rpx;
  color: #000000;
}
.parameterBox .parameterBox_main .right .uni-input {
  border: 1rpx solid #F2F2F2;
}
.parameterBox .parameterBox_main .bottom {
  width: 100%;
  padding: 30rpx 10rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #F0F0F0;
}
.parameterBox .parameterBox_main .bottom .bottom_right_title image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.parameterBox .parameterBox_main .bottom .bottom_left_title {
  width: 160rpx;
  display: inline-block;
  color: #000000;
  text-align: right;
}
.parameterBox .parameterBox_main .bottom checkbox-group {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.parameterBox .parameterBox_main .bottom radio-group {
  width: auto;
}
.parameterBox .parameterBox_main .btnBox {
  padding: 20rpx 40rpx;
  margin-top: 40rpx;
}
.parameterBox .parameterBox_main .btnBox button {
  background: #409eff;
  color: #FFFFFF;
}

