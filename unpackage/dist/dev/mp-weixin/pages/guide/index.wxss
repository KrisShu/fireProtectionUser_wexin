.guideBox .logoBox {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.guideBox .logoBox .logoimg {
  width: 160rpx;
  height: 140rpx;
}
.guideBox .logoBox .logoText {
  width: 420rpx;
  height: 220rpx;
}
.guideBox .logoBox .logoFont text {
  display: block;
  font-family: 'lishu';
}
.guideBox .textBox {
  padding: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.guideBox .textBox text {
  line-height: 2;
}
.guideBox .textBox text.texttewo {
  margin-top: 60rpx;
}
.guideBox .btnBox {
  padding: 0rpx 40rpx;
  margin-top: 40rpx;
}
.guideBox .btnBox button {
  background: #409eff;
  color: #FFFFFF;
}
.guideBox .btnBox navigator {
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  background: #cccccc;
  margin-top: 20rpx;
  border-radius: 5rpx;
  font-size: 18px;
  color: #FFFFFF;
}

